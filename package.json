{
  "name": "java-script-data-types",
  "version": "1.2.0",
  "description": "Math Tests, Nodejs, Education",
  "main": "app.js",
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "init:project": "npm install",
    "libs:reinstall": "rm -rf node_modules && npm run init:project",
    "client:linter": "grunt",
    "client:dev": "grunt dev && node app",
    "client:prod": "grunt production && node app",
    "client:watch": "grunt watch",
    "server:watch": "npx tsc --watch",
    "server:test": "npx tsc --listEmittedFiles && node app",
    "server:dev": "grunt dev && npx tsc --listEmittedFiles && node app",
    "server:prod": "grunt production && npx tsc --removeComments --listEmittedFiles && node app",
    "start:dev": "npm run server:dev",
    "start:prod": "npm run server:prod",
    "postinstall": "npm run start:dev",
    "start": "npm run libs:reinstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiagolimaa/java-script-data-types.git"
  },
  "keywords": [
    "Math",
    "Education",
    "Java",
    "Script"
  ],
  "author": "Thiago Lima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiagolimaa/java-script-data-types/issues"
  },
  "homepage": "https://github.com/thiagolimaa/java-script-data-types#readme",
